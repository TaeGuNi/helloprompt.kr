{
  "name": "aura-core",
  "version": "1.0.845",
  "description": "> **World-Class Skin Analysis Engine** combining Medical Logic, Lifestyle Genome, and AI Vision.",
  "main": "./dist/engine/index.js",
  "types": "./dist/engine/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "engines": {
    "node": "24.13.0"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "lint": "eslint '{engine,tests}/**/*.{ts,js}'",
    "format": "prettier --write '{engine,tests}/**/*.{ts,js}'",
    "batch": "tsx scripts/run-batch.ts",
    "build": "tsc",
    "prepare": "husky && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaeGuNi/aura-core.git"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/TaeGuNi/aura-core/issues"
  },
  "homepage": "https://github.com/TaeGuNi/aura-core#readme",
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "knip": "^5.83.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "puppeteer": "^24.37.3",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  }
}
